<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"   xmlns:h="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
	.panel{
		display: inline-block;
	}
	</ui:style>
	
	<g:AbsolutePanel styleName="{style.panel}">
		<g:CaptionPanel ui:field="header">
			<g:FlowPanel ui:field="panel">
				<g:Label ui:field="categoriesLabel" />
				<g:ListBox ui:field="categories" />
				<g:Label ui:field="registerCodeLabel" />
				<g:TextBox ui:field="registerCode" />
				<g:Label ui:field="effectiveUntilLabel" />
				<h:DateBox ui:field="effectiveUntil" />
			</g:FlowPanel>
		</g:CaptionPanel>
	</g:AbsolutePanel>
	
</ui:UiBinder> 